journal = {Cryptol. ePrint Arch.},
keywords = {classical,qds,signatures,theoretical},
mendeley-tags = {classical,qds,signatures,theoretical},
title = {{Unconditionally Secure Signatures}},
url = {http://eprint.iacr.org/2016/739},
year = {2016}
}
